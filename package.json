{
  "name": "holidays-jp",
  "description": "get Japanese public holidays",
  "version": "1.4.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "bin": {
    "holiday": "bin/holidays-jp.js",
    "holidays-jp": "bin/holidays-jp.js"
  },
  "browser": "dist/holidays-jp.js",
  "dependencies": {
    "japanese-public-holidays": "^3.0.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "csv-parse": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-node": "^11.0.0",
    "ltsv": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.0.0"
  },
  "files": [
    "bin/holidays-jp.js",
    "data.*",
    "index.d.ts",
    "index.js",
    "index.js.map",
    "index.mjs"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/holidays-jp.js.git"
  },
  "scripts": {
    "build": "run-p -l build:*",
    "build:cjs": "swc -C module.type=commonjs -o dist/index.cjs index.ts",
    "build:esm": "swc -C module.type=es6 -o dist/index.mjs index.ts",
    "build:tsc": "tsc",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix .",
    "fix:fixpack": "npx fixpack",
    "fix:prettier": "prettier --write .",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint .",
    "lint:fixpack": "npx fixpack --dryRun",
    "lint:prettier": "prettier --check .",
    "pretest": "run-s build:cjs",
    "pretest:browser": "run-s build:umd",
    "test": "mocha",
    "test:browser": "karma start --single-run"
  },
  "sideEffects": false,
  "types": "index.d.ts"
}
